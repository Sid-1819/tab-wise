{
  "manifest_version": 3,
  "name": "Tab Wise",
  "version": "1.1.0",
  "description": "Organize and manage your Chrome tabs efficiently",
  "permissions": ["tabs"],
  "host_permissions": ["<all_urls>"],

  "action": {
    "default_popup": "src/popup/popup.html",
    "default_title": "Tab Wise"
  },
  "icons": {
    "16": "icons/icon2.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["icons/*"],
      "matches": ["<all_urls>"]
    }
  ]
}