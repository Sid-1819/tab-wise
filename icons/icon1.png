<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 100 100" width="100" height="100">
  <!-- Circular background -->
  <circle cx="50" cy="50" r="50" fill="#1a1a1a"></circle>

  <!-- Centered content group -->
  <g transform="translate(30, 28)">
    <!-- Tab 3 (back) -->
    <rect x="20" y="20" width="40" height="8" rx="4" fill="#404040"></rect>
    
    <!-- Tab 2 (middle) -->
    <rect x="10" y="12" width="40" height="8" rx="4" fill="#606060"></rect>
    
    <!-- Tab 1 (front/active) -->
    <rect x="0" y="4" width="40" height="8" rx="4" fill="#ffffff"></rect>
    
    <!-- Minimize icon in tab 1 -->
    <circle cx="35" cy="8" r="1.5" fill="#1a1a1a"></circle>
    
    <!-- Organizing grid icon -->
    <g transform="translate(45, 30)">
      <rect x="0" y="0" width="4" height="4" fill="#ffffff"></rect>
      <rect x="6" y="0" width="4" height="4" fill="#ffffff"></rect>
      <rect x="0" y="6" width="4" height="4" fill="#ffffff"></rect>
      <rect x="6" y="6" width="4" height="4" fill="#ffffff"></rect>
    </g>
  </g>
</svg>
